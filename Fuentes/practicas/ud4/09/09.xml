<!DOCTYPE ejemplos [
	<!ELEMENT lenguaje (nLenguaje,codigo)>
	<!ELEMENT nLenguaje (#PCDATA)>
	<!ELEMENT codigo (#PCDATA)>
	<!ELEMENT ejemplos (lenguaje*)>
]>

<ejemplos>
	<lenguaje>
		<nLenguaje>Java</nLenguaje>
		<codigo>
			<![CDATA[
				public class holaMundo{
					public static void main(String[] args){
						System.out.println("Hola Mundo");
					}
				}
			]]>
		</codigo>
	</lenguaje>
	<lenguaje>
		<nLenguaje>HTML</nLenguaje>
		<codigo>
			<![CDATA[
				<!DOCTYPE html>
				<html>
					<body>
						<h1>Mi primer encabezado</h1>
						<p>Mi primer párrafo</p>
					</body>
				</html>
			]]>
		</codigo>
	</lenguaje>
</ejemplos>